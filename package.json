  "dependencies": {
    "axios": "^1.2.1",
    "element-closest-polyfill": "^1.0.5",
    "flowbite": "^1.5.4",
    "minze": "^1.0.3",
    "scrollreveal": "^4.0.9",
    "swiper": "^8.4.5"
  },
  "browserslist": [
    "last 3 versions",
    "> 5%",
    "iOS >= 9.0",
    "Android >= 5",
    "Firefox ESR"
